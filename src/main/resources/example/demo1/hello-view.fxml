<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>


<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="example.demo1.HelloController">
    <padding>
        <Insets bottom="20.0" left="25.0" right="20.0" top="20.0"/>
    </padding>

    <GridPane xmlns:fx="http://javafx.com/fxml" alignment="CENTER" hgap="70" vgap="70">
        <Button text="Veterinário" GridPane.rowIndex="0" GridPane.columnIndex="0"  prefHeight="200" prefWidth="300" style="-fx-font-size: 20px;" />
        <Button text="Pet" GridPane.rowIndex="0" GridPane.columnIndex="1" prefHeight="200" prefWidth="300" style="-fx-font-size: 20px;"/>
        <Button text="Vacinas" GridPane.rowIndex="1" GridPane.columnIndex="0" prefHeight="200" prefWidth="300" style="-fx-font-size: 20px;"/>
        <Button text="Usuário" GridPane.rowIndex="1" GridPane.columnIndex="1" prefHeight="200" prefWidth="300" style="-fx-font-size: 20px;"/>
    </GridPane>

</VBox>
